{
    "routes": [{ "src": "/(.*)", "dest": "/index.js" }],
    "env": {
        "PORT": "",
        "DB_USER": "@appflix-db-user",
        "DB_PASSWORD": "@appflix-db-password",
        "DB_HOST": "@appflix-db-host",
        "DB_NAME": "@appflix-db-name",
        "DEFAULT_ADMIN_PASSWORD": "@appflix-default-admin-password",
        "DEFAULT_USER_PASSWORD": "@appflix-default-user-password",
        "AUTH_JWT_SECRET": "@appflix-auth-jwt-secret",
        "PUBLIC_API_KEY_TOKEN": "@appflix-public-api-key-token",
        "ADMIN_API_KEY_TOKEN": "@appflix-admin-api-key-token"
    }
}
